{
  "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true,
      "node": true
  },
  "extends": [
      "airbnb-base"
  ],
  "parserOptions": {
      "ecmaVersion": 2021
  },
  "rules": {
    "linebreak-style": 0,
    "no-console": "off",
    "no-underscore-dangle": [
      "error",
      { "allow": ["_id"] }
    ]
  }
}
